{
    "version": 2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "*.js": {
            "regions": ["sin1"]  // Singapore region for lower latency in India
        }
    }
}
